{
  "manifest_version": 2,
  "name": "Just Do It Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["https://www.facebook.com/*", "http://www.facebook.com/*"],
    "js": ["jquery-2.1.4.js", "content.js"],
    "css": ["style.css"]
    }],
  "browser_action": {
    "default_icon": "off.png"
  }
}
